{
  "manifest_version": 2,

  "name": "Enemy count from XC",
  "description": "This extension return the count of enemies",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "<all_urls>"
  ]
}